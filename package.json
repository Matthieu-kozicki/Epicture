{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "doc": "vuedoc.md src/views/*.vue --output doc/ && vuedoc.md src/widgets/Currency/*.vue --output doc/ && vuedoc.md src/widgets/Imgur/*.vue --output doc/ && vuedoc.md src/widgets/Spotify/*.vue --output doc/ && vuedoc.md src/widgets/Weather/*.vue --output doc/ && vuedoc.md src/widgets/Youtube/*.vue --output doc/"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "firebase": "^8.0.2",
    "firebaseui": "^4.7.1",
    "mitt": "^2.1.0",
    "primeicons": "^4.1.0",
    "primevue": "^3.0.1",
    "sortablejs": "^1.12.0",
    "v-movable": "^0.3.2",
    "vue": "^3.0.3",
    "vue-draggable-next": "^2.0.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "jsdoc": "^3.6.6",
    "jsdoc-vuejs": "^3.0.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "vue/no-unused-vars": "off",
      "no-useless-escape": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
